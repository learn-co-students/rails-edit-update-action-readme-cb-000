<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit Article</title>
  </head>
  <body>
    <%= form_for @article do |f| %>
     <%= f.label 'Article Title' %> <br>
     <%= f.text_field :title %> <br>

     <%= f.label 'Article Description' %> <br>
     <%= f.text_field :description %> <br>

     <%= f.submit 'Submit Article' %>
    <% end %>
  </body>
</html>
